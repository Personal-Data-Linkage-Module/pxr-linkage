                "summary": "CToken件数検索",
                "description": "CTokenの件数を検索します。",
                "requestBody": {
                    "description": "検索条件",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "description": "検索条件のリクエストボディ",
                                "properties": {
                                    "createAt": {
                                        "type": "object",
