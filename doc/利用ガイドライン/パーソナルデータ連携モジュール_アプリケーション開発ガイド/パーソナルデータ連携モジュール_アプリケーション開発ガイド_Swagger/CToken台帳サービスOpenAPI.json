                                        "properties": {
                                            "start": {
                                                "type": "string",
                                                "description": "検索開始CToken作成時間"
                                            },
                                            "end": {
                                                "type": "string",
                                                "description": "検索終了CToken作成時間"
                                            }
                                        }
                                    },
                                    "pxrId": {
                                        "type": "string",
                                        "description": "PxR-ID"
                                    },
                                    "document": {
                                        "$ref": "#/components/schemas/_codeOnlyValue"
                                    },
                                    "event": {
                                        "$ref": "#/components/schemas/_codeOnlyValue"
                                    },
                                    "thing": {
                                        "$ref": "#/components/schemas/_codeOnlyValue"
                                    }
                                },
                                "example": {
                                    "createAt": {
                                        "start": "2020-01-01T00:00:00.000+0900",
                                        "end": "2020-09-01T00:00:00.000+0900"
                                    },
                                    "pxrId": null,
                                    "document": [],
                                    "event": [],
                                    "thing": []
                                }
                            }
                        }
                    }
                },
                "responses": {
