{
    "openapi": "3.0.2",
    "info": {
        "title": "CToken台帳サービス",
        "version": "1.0",
        "description": "CToken台帳の管理を行います。"
    },
    "servers": [
        {
            "url": "/ctoken-ledger"
        }
