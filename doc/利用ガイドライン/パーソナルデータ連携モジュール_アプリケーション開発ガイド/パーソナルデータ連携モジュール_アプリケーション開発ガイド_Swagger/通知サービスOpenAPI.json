                                            "noticeUrl": {
                                                "type": "string",
                                                "description": "通知先URL"
                                            },
                                            "expirationAt": {
                                                "type": "string",
                                                "description": "有効期限",
                                                "format": "yyyy-MM-dd hh:mm:ss.SSS"
                                            }
                                        }
                                    }
                                }
                            },
                            "example": {
                                "type": 1,
                                "title": "承認リクエスト",
                                "content": "テスト: 承認をリクエストします。",
                                "attribute": {
                                    "testKey": "testValue"
