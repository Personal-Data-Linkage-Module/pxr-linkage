                                    },
                                    "プロパティが範囲外です": {
                                        "value": {
                                            "status": 400,
                                            "message": "<プロパティ名>が範囲外です。"
                                        }
                                    },
                                    "文字列指定プロパティが文字列ではありません": {
                                        "value": {
                                            "status": 400,
                                            "reasons": [
                                                {
                                                    "property": "<プロパティ名>",
                                                    "value": "<入力値>",
                                                    "message": "文字列ではありません"
                                                }
                                            ]
                                        }
                                    },
                                    "数値指定プロパティが数値ではありません": {
